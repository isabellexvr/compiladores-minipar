# Divisao via string
c_channel calculadora computador_1 computador_2

COMP computador_1
SEQ
  operacao = "/";
  valor1 = 20;
  valor2 = 5;
  resultado = 0;
  calculadora.send(operacao, valor1, valor2, resultado);

COMP computador_2
SEQ
  calculadora.receive(operacao, valor1, valor2, resultado);
  print resultado;
