fun xor_logic(a, b) {
  SEQ
    # XOR: 1 se diferentes, 0 se iguais
    if (a == b) SEQ return 0;
    else SEQ return 1;
}

SEQ
  print "=== TESTE XOR COM MATRIZES ===";
  entradas = [[0, 0], [0, 1], [1, 0], [1, 1]];
  saidas_desejadas = [0, 1, 1, 0];
  i = 0;
  while (i < 4) SEQ
    entrada1 = entradas[i][0];
    entrada2 = entradas[i][1];
    saida_esperada = saidas_desejadas[i];
  resultado = xor_logic(entrada1, entrada2);
    print "Caso", i, ":", entrada1, entrada2, "-> Esperado:", saida_esperada, "Obtido:", resultado;
    i = i + 1;

print "Fim do teste XOR com matrizes";
